<table class="table table-striped table-hover table-condensed table-bordered table-responsive" id="theme_table">
    <tr >
        <th scope="col">Id</th>
        <th scope="col">Theme</th>

    </tr>
    {{#each themes}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{id}}{{theme}}</td>

        </tr>
    {{/each}}
</table>
{{> table_pagination}}