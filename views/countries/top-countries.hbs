<div class="container mt-4">
    <h1>Top 10 Paesi</h1>
    <table class="table table-striped table-bordered">
        <thead>
        <tr>
            <th>Position</th>
            <th>Country</th>
            <th>Number of Movies</th>
        </tr>
        </thead>
        <tbody>
        {{#each countries}}
            <tr>
                <td>{{@index}}</td>
                <td>{{country}}</td>
                <td>{{movie_count}}</td>
            </tr>
        {{/each}}
        </tbody>
    </table>
</div>
